{
  "app": {
    "name": "DADI.Cloud API"
  },
  "auth": {
    "database": "cloud",
    "datastore": "@dadi/api-mongodb"
  },
  "caching": {
    "directory": {
      "enabled": false,
      "path": "./cache/api"
    },
    "redis": {
      "enabled": false
    }
  },
  "cors": true,
  "datastore": "@dadi/api-mongodb",
  "env": "development",
  "logging": {
    "level": "debug",
    "accessLog": {
      "enabled": true
    },
    "stream": "stdout"
  },
  "media": {
    "basePath": "../web/workspace/public",
    "storage": "disk"
  },
  "paths": {
    "collections": "workspace/collections",
    "endpoints": "workspace/endpoints",
    "hooks": "workspace/hooks"
  },
  "publicUrl": {
    "host": "127.0.0.1",
    "port": 8081
  },
  "server": {
    "host": "127.0.0.1",
    "port": 8081,
    "protocol": "http"
  }
}